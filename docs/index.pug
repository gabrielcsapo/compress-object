                            i Compress Object
                        br
                        div
                            div#compress-object.codeblock.
                                var compress = require('compress-object');

                                var flattened = compress({
                                    name: '',
                                    age: 0,
                                    gender: ''
                                }).serialize({
                                    name: 'Gabriel J. Csapo',
                                    age: 21,
                                    gender: 'Male'
                                });

                                console.log(JSON.stringify(flattened, null, 4));
                        br
                        br
                        div(class="text-left text-black")
                            i Deserialize Object
                        br
                        div
                            div#deserialize-object.codeblock.
                                var compress = require('compress-object');

                                var flattened = [ 'Gabriel J. Csapo', 21, 'Male' ];


                                var object = compress({
                                    name: '',
                                    age: 0,
                                    gender: ''
                                }).deserialize(flattened);


                                console.log(JSON.stringify(object, null, 4));
